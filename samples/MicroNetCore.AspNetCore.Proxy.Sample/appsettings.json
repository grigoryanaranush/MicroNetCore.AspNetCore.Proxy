{
  "ProxyOptions": {
    "api/posts": "[jsonPlaceholder]/posts",
    "api/comments": "[jsonPlaceholder]/comments",
    "api/albums": "[jsonPlaceholder]/albums",
    "api/photos": "[jsonPlaceholder]/photos",
    "api/todos": "[jsonPlaceholder]/todos",
    "api/users": "[jsonPlaceholder]/users"
  },
  "ServiceOptions": {
    "jsonPlaceholder": "http://jsonplaceholder.typicode.com"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}